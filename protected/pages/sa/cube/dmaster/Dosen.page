                                    $('divAddNIDN').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddNIDNValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddNIDN').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                            <com:TActiveCustomValidator ID="addNIDN" ControlToValidate="txtAddNIDN" Display="Dynamic" OnServerValidate="checkNIDN" ValidationGroup="addDosen" ErrorMessage="NIDN / NIDK Yang Anda Inputkan Sudah Ada" FocusOnError="true">
                                <prop:ClientSide.OnValidationError>
                                    $('divAddNIDN').addClassName('has-error');
                                    jQuery('#<%=$this->addNIDN->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddNIDN').removeClassName('has-error');
