                                <prop:ClientSide.OnValidationError>
                                    $('divAddNIPY').addClassName('has-error');
                                    jQuery('#<%=$this->FieldAddNIPYValidator->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddNIPY').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TRequiredFieldValidator>
                            <com:TActiveCustomValidator ID="addNIPY" ControlToValidate="txtAddNIPY" Display="Dynamic" OnServerValidate="checkNIPY" ValidationGroup="addDosen" ErrorMessage="NIPY Yang Anda Inputkan Sudah Ada" FocusOnError="true">
                                <prop:ClientSide.OnValidationError>
                                    $('divAddNIPY').addClassName('has-error');
                                    jQuery('#<%=$this->addNIPY->ClientID%>').removeAttr('style');
                                </prop:ClientSide.OnValidationError>
                                <prop:ClientSide.OnValidationSuccess>
                                    $('divAddNIPY').removeClassName('has-error');
                                </prop:ClientSide.OnValidationSuccess>
                            </com:TActiveCustomValidator>
                        </div>
                    </div>
                    <div class="form-group" id="divAddNama">
                        <label class="col-lg-2 control-label">Nama :</label>
