<%@ Title="Formulir Pendaftaran" %>
<com:TContent ID="moduleheader">
    <i class="fa fa-file"></i> Formulir Pendaftaran <com:TActiveLabel ID="lblModulHeader" />
</com:TContent>
<com:TContent ID="modulebreadcrumb">    
    <li class="active"><span>FORMULIR PENDAFTARAN</span></li>
</com:TContent>
<com:TContent ID="maincontent">	    
<com:TPanel Visible="<%= $this->getViewProcess(false) %>" CssClass="row">                    
    <div class="col-lg-12">
        <div class="main-box clearfix">               
            <header class="main-box-header clearfix">
            </header>
            <div class="main-box-body">
                <div class="alert alert-warning">
                    <i class="fa fa-info-circle fa-fw fa-lg"></i>
                    <strong>
                        Info!
                    </strong>
                     <com:TLiteral ID="errorMessage" />
                </div>
            </div>            
        </div>
    </div>
</com:TPanel>
<com:TPanel Visible="<%= $this->getEditProcess(false)%>" CssClass="row">	
    <com:THiddenField ID="hiddenemail" />          
