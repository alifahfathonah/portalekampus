<%@ Title="Daftar Ulang - Mahasiswa Drop Out"%>
<com:TContent ID="moduleheader">
    <i class="fa fa-location-arrow"></i> Daftar Ulang Mahasiswa Drop Out <br/><small><com:TActiveLabel ID="lblModulHeader" /></small>
</com:TContent>
<com:TContent ID="modulebreadcrumb">    
    <li><a href="#">DAFTAR ULANG</a></li>    
    <li class="active"><span>MAHASISWA LULUS</span></li>
</com:TContent>
<com:TContent ID="maincontent">
<com:TPanel Visible="<%= $this->getViewProcess(false) %>" CssClass="row">                    
    <div class="col-lg-12">
        <div class="main-box clearfix">               
            <header class="main-box-header clearfix">
                <div class="icon-box pull-right">                                       
                    <a class="btn pull-left" href="<%=$this->constructUrl('dulang.DulangMHSDropOut',true)%>">
                        <i class="fa fa-times"></i>
                    </a>
                </div>
            </header>
            <div class="main-box-body">
                <div class="alert alert-warning">
                    <i class="fa fa-info-circle fa-fw fa-lg"></i>
                    <strong>
                        Info!
                    </strong>
                     <com:TLiteral ID="errorMessage" />
                </div>
            </div>            
        </div>
    </div>
</com:TPanel>
<com:TPanel Visible="<%= $this->getDefaultProcess() %>"> 
    <%include Application.layouts.cube.DataMHS%>
    <div class="row">
        <div class="col-lg-12">
            <div class="main-box clearfix">
                <header class="main-box-header clearfix">
                    <h2 class="pull-left"><i class="fa fa-location-arrow"></i> Dinyatakan Drop Out dengan Data Berikut :</h2>                    
                    <div class="filter-block pull-right">                                                   
                        <com:TActiveLinkButton ID="btnClose" OnClick="closeDetailDulang" ClientSide.PostState="false" CssClass="btn btn-default pull-left" Attributes.Title="Keluar">
                            <prop:Text><i class="fa fa-times fa-lg"></i></prop:Text>
                            <prop:ClientSide.OnPreDispatch>
                                $('loading').show();
                            </prop:ClientSide.OnPreDispatch>
                            <prop:ClientSide.OnLoading>
                                $('<%=$this->btnClose->ClientId%>').disabled='disabled';				
                            </prop:ClientSide.OnLoading>
                            <prop:ClientSide.OnComplete>
                                $('<%=$this->btnClose->ClientId%>').disabled='';				
                                $('loading').hide();
                            </prop:ClientSide.OnComplete>
                        </com:TActiveLinkButton>
                    </div>
                </header>
                <div class="main-box-body clearfix">
                    <div class="form-horizontal"> 
                        <div class="form-group" id="divAddTADropOut">
                            <label class="col-lg-2 control-label">T.A / SMT Drop Out :</label>
                            <div class="col-lg-10">
                                <div class="row">
                                    <div class="col-lg-2">
                                        <com:TDropDownList ID="cmbAddTADropOut" CssClass="form-control" />
                                    </div>
                                    <div class="col-lg-2">
                                        <com:TDropDownList ID="cmbAddSMTDropOut" CssClass="form-control" />
                                    </div>
                                </div>                                
                            </div>
                        </div>
                        <div class="form-group" id="divAddTADropOut">
                            <label class="col-lg-2 control-label">Jumlah SKS :</label>
                            <div class="col-lg-10">
                                <p class="form-control-static"><com:TLiteral ID="literalJumlahSKS" /></p>
                            </div>
                        </div>
                        <div class="form-group" id="divAddTADropOut">
                            <label class="col-lg-2 control-label">Bebas Keuangan :</label>
                            <div class="col-lg-10">
                                <p class="form-control-static"><com:TLiteral ID="literalBebasKeuangan" /></p>
                            </div>
                        </div>
                        <div class="form-group" id="divAddTADropOut">
                            <label class="col-lg-2 control-label">Bebas Perpustakaan :</label>
                            <div class="col-lg-10">
                                <p class="form-control-static"><com:TLiteral ID="literalBebasPerpustakaan" /></p>
                            </div>
                        </div>                                     
                        <div class="form-group">
                            <div class="col-lg-offset-2 col-lg-10">
                                <com:TActiveCustomButton ID="btnSave" CssClass="btn btn-primary" OnClick="saveData" ValidationGroup="addDulangMhsDropOut">
                                    <prop:Text><i class="fa fa-save"></i> Simpan</prop:Text>
                                    <prop:ClientSide.OnPreDispatch>                                                                   
                                        $('loading').show(); 
                                        $('<%=$this->btnSave->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnPreDispatch>
                                    <prop:ClientSide.OnLoading>
                                        $('<%=$this->btnSave->ClientId%>').disabled='disabled';						
                                    </prop:ClientSide.OnLoading>
                                    <prop:ClientSide.OnComplete>																	                                    						                                                                            
                                        $('<%=$this->btnSave->ClientId%>').disabled='';
                                        $('loading').hide(); 
                                    </prop:ClientSide.OnComplete>
                                </com:TActiveCustomButton>
                            </div>
                        </div>   
                    </div>
                </div>
            </div>
        </div>
    </div>
</com:TPanel>
</com:TContent>